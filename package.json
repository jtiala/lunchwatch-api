{
  "name": "lunchwatch-api",
  "version": "0.2.0",
  "description": "LunchWatch Menu Aggregator API",
  "author": "Joonas Tiala <joonas.tiala@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtiala/lunchwatch-api"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && NODE_ENV=production tsc",
    "start": "NODE_ENV=production node dist/src/server.js",
    "start:dev": "nodemon --inspect=0.0.0.0:9229 --nolazy src/server.ts",
    "start:pm2": "pm2 stop dist/src/server.js; NODE_ENV=production pm2 start dist/src/server.js --name=\"LunchWatch API\"",
    "format": "prettier --write '**/*.{ts,js,json,css,scss,html,md,yml,yaml}'",
    "lint:ts": "eslint src/**/*.ts",
    "lint:ts:fix": "eslint src/**/*.ts --fix",
    "lint:other": "prettier --list-different '**/*.{json,md,yml,yaml}'",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:fix": "yarn lint:ts:fix && format",
    "test": "echo \"No tests yet\"",
    "migrate": "knex migrate:latest --knexfile src/database/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/database/knexfile.ts",
    "migrate:rollback:all": "knex migrate:rollback --all --knexfile src/database/knexfile.ts",
    "migrate:up": "knex migrate up --knexfile src/database/knexfile.ts",
    "migrate:down": "knex migrate down --knexfile src/database/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/database/knexfile.ts",
    "seed": "knex seed:run --knexfile src/database/knexfile.ts",
    "seed:make": "knex seed:make --knexfile src/database/knexfile.ts"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "apollo-cursor-pagination": "^0.5.1",
    "apollo-server-express": "^2.8.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "helmet": "^3.20.0",
    "knex": "^0.19.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "p-queue": "^6.1.1",
    "pg": "7.12.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.0.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/hapi__boom": "^7.4.0",
    "@types/helmet": "^0.0.43",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.1",
    "@types/node-fetch": "^2.5.0",
    "@types/node-schedule": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.7.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
}
