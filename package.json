{
  "name": "lunchwatch-api",
  "version": "0.2.0",
  "description": "LunchWatch Menu Aggregator API",
  "author": "Joonas Tiala <joonas.tiala@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtiala/lunchwatch-api"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --mode production",
    "start": "NODE_ENV=production node build/src/index.js",
    "start:dev": "nodemon --inspect=0.0.0.0:9229 --nolazy src/index.ts",
    "format": "prettier --write '**/*.{ts,js,json,css,scss,html,md,yml,yaml}'",
    "lint:ts": "eslint src/**/*.ts",
    "lint:ts:fix": "eslint src/**/*.ts --fix",
    "lint:other": "prettier --list-different '**/*.{json,md,yml,yaml}'",
    "lint": "npm run lint:ts && npm run lint:other",
    "lint:fix": "npm run lint:ts:fix && format",
    "test": "echo \"No tests yet\"",
    "migrate": "knex migrate:latest --knexfile src/database/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile src/database/knexfile.ts",
    "migrate:rollback:all": "knex migrate:rollback --all --knexfile src/database/knexfile.ts",
    "migrate:up": "knex migrate:up --knexfile src/database/knexfile.ts",
    "migrate:down": "knex migrate:down --knexfile src/database/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/database/knexfile.ts",
    "seed": "knex seed:run --knexfile src/database/knexfile.ts",
    "seed:make": "knex seed:make --knexfile src/database/knexfile.ts"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "apollo-cursor-pagination": "^0.5.1",
    "apollo-server-express": "^2.9.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.2.1",
    "deepmerge": "^4.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.6",
    "he": "^1.2.0",
    "helmet": "^3.21.0",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.7",
    "node-schedule": "^1.3.2",
    "p-queue": "^6.1.1",
    "pg": "7.12.1",
    "puppeteer-core": "^1.20.0",
    "striptags": "^3.1.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/hapi__boom": "7.4.1",
    "@types/he": "^1.1.0",
    "@types/helmet": "^0.0.44",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.5",
    "@types/node-fetch": "^2.5.0",
    "@types/node-schedule": "^1.2.4",
    "@types/puppeteer-core": "^1.9.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  }
}
